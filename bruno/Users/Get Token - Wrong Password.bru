meta {
  name: Get Token - Wrong Password
  type: http
  seq: 6
}

post {
  url: {{baseUrl}}/users/tokens
  body: json
  auth: none
}

body:json {
  {
    "email": "{{userEmail}}",
    "password": "wrongpassword"
  }
}

tests {
  test("should return 400 with the wrong password", function() {
    const data = res.getBody();
    expect(res.getStatus()).to.equal(400);
    expect(data).to.have.property("message");
  });
}
